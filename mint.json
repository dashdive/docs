{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Dashdive",
  "logo": {
    "dark": "/logo/logo.png",
    "light": "/logo/logo.png"
  },
  "api": {
    "baseUrl": "https://ingest.dashdive.com",
    "auth": {
      "method": "key",
      "name": "X-API-Key"
    },
    "playground": {
      "mode": "show"
    }
  },
  "openapi": ["openapi.json"],
  "favicon": "/favicon.png",
  "colors": {
    "primary": "#006DC2",
    "light": "#ecf5fd",
    "dark": "#064c86",
    "anchors": {
      "from": "#006DC2",
      "to": "#4896D4"
    }
  },
  "topbarLinks": [
    {
      "name": "Support",
      "url": "mailto:support@dashdive.com"
    }
  ],
  "topbarCtaButton": {
    "name": "Dashboard",
    "url": "https://app.dashdive.com"
  },
  "tabs": [
    {
      "name": "API Reference",
      "url": "api-reference"
    }
  ],
  "anchors": [
    {
      "name": "Documentation",
      "icon": "book-open-cover",
      "url": "https://docs.dashdive.com"
    },
    {
      "name": "Blog",
      "icon": "newspaper",
      "url": "https://www.dashdive.com/blog"
    },
    {
      "name": "Chat with Us",
      "icon": "phone",
      "url": "https://calendly.com/micah-wheat/dashdive-chat?month=2023-11"
    }
  ],
  "navigation": [
    {
      "group": "Get Started",
      "pages": ["introduction", "how-dashdive-works", "quickstart", "events"]
    },
    {
      "group": "S3",
      "pages": ["s3/overview", "s3/integration"]
    },
    {
      "group": "API Documentation",
      "pages": ["api-reference/introduction"]
    },
    {
      "group": "Endpoint",
      "pages": ["api-reference/endpoint/s3"]
    }
  ],
  "footerSocials": {
    "github": "https://github.com/dashdive",
    "linkedin": "https://www.linkedin.com/company/dashdive"
  }
}
